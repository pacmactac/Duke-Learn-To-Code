// Panda border practice 
var Panda = new SimpleImage("smallpanda.png");
for (var pixel of Panda.values()) {
    var x = pixel.getX();
    var y = pixel.getY();
    if (pixel.getX() <= 10) {
        pixel.setGreen(0);
        pixel.setBlue(0);
        pixel.setRed(0);
    }
    if (pixel.getY() <= 10) {
        pixel.setGreen(0);
        pixel.setBlue(0);
        pixel.setRed(0);
    }
    if (pixel.getX() >= Panda.getWidth() - 10) {
        pixel.setGreen(0);
        pixel.setBlue(0);
        pixel.setRed(0);
    }
    if (pixel.getY() >= Panda.getHeight() - 10) {
        pixel.setGreen(0);
        pixel.setBlue(0);
        pixel.setRed(0);
    }
}

print(Panda)
